# client setting
CAUTH_CLIENT_ID=
CAUTH_CLIENT_SECRET=
CAUTH_CALLBACK=

#authorize type
CAUTH_RESPONSE_TYPE=code
CAUTH_GRANT_TYPE=authorization_code

# authorization uri
CAUTH_SITE=

CAUTH_AUTHORIZE_PATH=/oauth/authorize
CAUTH_AUTHORIZED_APPLICATIONS_PATH=/oauth/authorized_applications
CAUTH_TOKEN_PATH=/oauth/token
CAUTH_TOKEN_INFO_PATH=/oauth/token/info
CAUTH_SIGNIN_PATH=/account/sign_in
CAUTH_SIGNOUT_PATH=/account/sign_out
CAUTH_REVOKE_PATH=/oauth/revoke
CAUTH_OWNER_INFO_PATH=/api/v1/account

CAUTH_SIGNIN_URL="$CAUTH_SITE$CAUTH_SIGNIN_PATH"
CAUTH_SIGNOUT_URL="$CAUTH_SITE$CAUTH_SIGNOUT_PATH"
CAUTH_AUTHORIZE_URL="$CAUTH_SITE$CAUTH_AUTHORIZE_PATH"
CAUTH_AUTHORIZED_APPLICATIONS_URL="$CAUTH_SITE$CAUTH_AUTHORIZED_APPLICATIONS_PATH"
CAUTH_NEW_AUTHORIZE_URL="$CAUTH_AUTHORIZE_URL?response_type=$CAUTH_RESPONSE_TYPE&client_id=$CAUTH_CLIENT_ID&redirect_uri=$CAUTH_CALLBACK"
CAUTH_TOKEN_URL="$CAUTH_SITE$CAUTH_TOKEN_PATH"
CAUTH_TOKEN_INFO_URL="$CAUTH_SITE$CAUTH_TOKEN_INFO_PATH"
CAUTH_REVOKE_URL="$CAUTH_SITE$CAUTH_REVOKE_PATH"
CAUTH_OWNER_INFO_URL="$CAUTH_SITE$CAUTH_OWNER_INFO_PATH"
